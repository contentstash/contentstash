{
  "name": "contentstash/core",
  "version": "0.0.1",
  "description": "Core package for ContentStash, a headless CMS based on Laravel, Inertia.js, Vue 3, and Tailwind CSS.",
  "keywords": [
    "contentstash",
    "headless",
    "cms",
    "laravel",
    "inertia",
    "vue",
    "tailwind",
    "content management"
  ],
  "license": "MIT",
  "homepage": "https://github.com/contentstash/contentstash/blob/main/README.md",
  "support": {
    "issues": "https://github.com/contentstash/contentstash/issues"
  },
  "authors": [
    {
      "name": "Titus Kirch",
      "email": "contact@kirch.dev",
      "role": "Author"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/contentstash/contentstash"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.9",
    "inertiajs/inertia-laravel": "2.x-dev",
    "tightenco/ziggy": "^2.3",
    "spatie/laravel-model-info": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ContentStash\\Core\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "PluginRegistry": "ContentStash\\Core\\Facades\\PluginRegistryFacade"
      },
      "providers": [
        "ContentStash\\Core\\ContentStashServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.18"
  }
}
